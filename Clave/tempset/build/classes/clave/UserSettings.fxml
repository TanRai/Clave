   <children>
      <Label layoutX="141.0" layoutY="35.0" text="Change profile picture" />
      <ImageView fx:id="shownImage" fitHeight="120.0" fitWidth="120.0" layoutX="140.0" layoutY="86.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="uploadButton" layoutX="163.0" layoutY="252.0" mnemonicParsing="false" onAction="#uploadButtonAction" text="Upload file" />
      <Button fx:id="saveButton" layoutX="156.0" layoutY="313.0" mnemonicParsing="false" onAction="#saveButtonAction" text="Save changes" />
      <Label fx:id="fileLabel" alignment="CENTER" layoutX="1.0" layoutY="228.0" prefHeight="17.0" prefWidth="400.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
